{
  "name": "html-tailwindcss-boilerplate",
  "version": "1.0.0",
  "description": "a html tailwindcss boilerplate",
  "author": "Jeremy Dayniel David",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production postcss tailwind.css -o src/assets/css/style.css",
    "start": "postcss tailwind.css -o src/assets/css/style.css && live-server src"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.7.2"
  }
}
